{
  "name": "xxicatalog",
  "private": true,
  "description": "Interactive xxi publishers catalog",
  "repository": "temaput/xxicatalog",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run update-schema && webpack-dev-server --port 3000",
    "start-outer": "npm run update-schema && webpack-dev-server --port 3000 --host 10.0.0.55",
    "update-schema": "../manage.py graphql_schema --schema xxicatalog.schema --out ../xxicatalog/schema.json",
    "deploy-static": "cp -R public ../nginx/ && NODE_ENV=production webpack --output-path=../nginx/public/js --output-filename=app.js"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "0.9.2",
    "classnames": "2.2.5",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "graphql": "0.6.2",
    "graphql-relay": "0.4.2",
    "imports-loader": "^0.6.5",
    "material-design-lite": "^1.2.0",
    "material-ui": "^0.15.4",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-relay": "0.9.2",
    "react-router": "^2.7.0",
    "react-router-relay": "^0.13.4",
    "react-tap-event-plugin": "^1.0.0",
    "react-waypoint": "^3.1.1",
    "require-clean": "0.1.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "webpack-dev-server": "^1.14.1"
  }
}
